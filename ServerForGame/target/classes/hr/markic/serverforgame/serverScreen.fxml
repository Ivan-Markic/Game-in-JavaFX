<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.markic.serverforgame.controllers.ServerScreenController">
   <children>
      <Button fx:id="btnStart" layoutX="220.0" layoutY="373.0" mnemonicParsing="false" onAction="#StartServer" prefHeight="42.0" prefWidth="179.0" style="-fx-background-color: green;" text="Start Server">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
      <Button fx:id="btnExit" layoutX="221.0" layoutY="425.0" mnemonicParsing="false" onAction="#ExitApp" style="-fx-background-color: red;" text="Shutdown Server">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
